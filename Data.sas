*==================================================*;
* フォーマット ;
*==================================================*;

proc format;
  value Drugf 0='CONTROL' 1='6-MP';
  value Treatf 0='術中照射なし' 1='術中照射あり'; 
  value Stagef 3='ステージ3' 4='ステージ4';
  value Dosecf 10='10 nmol' 30='30 nmol' 90='90 nmol';
  value Modelf 1='(i) のモデル' 2='(ii) のモデル' 3='(iii) のモデル' 4='(iv) のモデル';
  value Model_Scancerf 0='カプラン・マイヤー' 1='(i) ダミー変数によるモデル'
                   2='(ii) 生の用量に対する線型モデル' 3='(iii) 対数用量に対する線型モデル';
  value Groupf 1='A' 2='B' 3='C' 4='D';
  value Method_survf 1='(i) 生存関数 (カプラン・マイヤー)' 2='(iii) 生存関数 (比例ハザードモデル)';
  value Method_llsf 1='(ii) 2重対数 (カプラン・マイヤー)' 2='(iv) 2重対数 (比例ハザードモデル)';
run;

